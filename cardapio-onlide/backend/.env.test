# backend/.env.test - VERSÃO CORRIGIDA
NODE_ENV=test
PORT=0

# Banco de Teste (SQLite em memória)
DB_DIALECT=sqlite
DB_STORAGE=:memory:

# JWT
JWT_SECRET=test-jwt-secret-123
JWT_EXPIRES_IN=1h

# Desabilitar logs em teste
SEQUELIZE_LOGGING=false
LOG_LEVEL=error

# Configurações específicas para teste
TEST_TIMEOUT=30000
DISABLE_REAL_DATABASE=true